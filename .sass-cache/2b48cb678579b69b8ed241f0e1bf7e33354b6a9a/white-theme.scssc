BODY{
	font-family: 'Helvetica Neue', Arial, 'Lucida Grande', sans-serif;
}

//background
.xcp-viewport {
	background:none;
}

/* Header Text Style */

H1,H2,H3,.x-panel-header-text-xcp-column-box-ui{
	font-family:  Arial, Helvetica, sans-serif;
	font-size: 13px
}
H1{
	font-size: 20px;
	font-weight: bold;
	color: #39C;
	line-height: 29px;
}

H2{
	font-size: 16px;
	font-weight: normal;
	color: #39C;
	line-height: 25px;
}

H3{
	font-size: 14px;
	font-weight: bold;
	color: #787878;
	padding-bottom: 4px;
	line-height: 24px;
}

H4{
	font-weight: bold;
	color: #787878;
	padding-bottom: 4px;
	line-height: 22px;
}

// Paragragh Text Style 
.xcp_plain_text-cls, .xcp_rich_text-cls,.x-form-item,.x-grid-row .x-grid-cell,.x-column-header,.x-body {
	font-family: 'Helvetica Neue', Arial, 'Lucida Grande', sans-serif;
	font-size: 12px;
	line-height: 21px;
}


//ColumnBox Updates

// .x-panel-xcp-column-box-ui{
//     border: 1px solid #D7D7D7;
// 	@include border-radius(6px);
// 	@include box-shadow(none);
// }

.x-panel-header{
		margin: 0!important;
		
}

.x-panel-header-default {
	border:none!important;
	@include box-shadow(none);
}

.x-panel-header-xcp-column-box-ui{
	background-image: none;
	@include background-image(linear-gradient(rgb(236,236,236) 30%, #F8F8F8 94%)); 
	background-color: #F8F8F8;
	border: 1px solid #D7D7D7!important;
	border-bottom-width: 0!important;
	padding: 6px;
	height: 34px;
	@include border-top-radius(6px);
}

.x-panel-header-text-xcp-column-box-ui{
	//color:#4e9ac9;
	//color: #363636;
	font-size: 13px;
}

.x-panel-header-xcp-column-box-ui + .x-panel-body-xcp-column-box-ui {
	background-color:#FFF;
	@include border-bottom-radius(6px);
	border: 1px solid #D7D7D7!important;
	border-top:none;
}

//General columnbox fix
.xcp_form-cls > .x-panel-body > .xcp_column_box-cls {
	margin-bottom: 10px;
}


// .x-panel-body-default {
// 	background: #fff;
// }


/******************************
 NAVIGATION MENU             
*/
// $xcp-app-header-background-gradient: color_stops(#444 0%, #222 100%);
// $xcp-app-menu-label-text-font-family: Arial, Helvetica, sans-serif;
// $xcp-app-menu-label-text-font-size: 16px;			
// $xcp-app-menu-label-text-font-color: #FFF;
// $xcp-header-tab-background-disabled: transparent;
// @include xcp-toolbar-ui('xcp-nav-toolbar',$background-gradient: $xcp-app-menu-background-gradient);


.xcp-master-header > .x-panel-body-xcp-column-box-ui,.xcp-master-header > * > .x-box-inner  {
     background-image: none;
      background-color: #444;
}

// 333 3F3F3F border-top: 1px solid #4E4E4E;
// border-bottom: 1px solid #393939;

.x-toolbar-xcp-nav-toolbar .x-btn-xcp-nav-button-toolbar-small-over,
.x-toolbar-xcp-nav-toolbar .x-btn-xcp-nav-button-toolbar-small-focus,
.xcp-userButton.over,
.xcp-userButton.over.x-menu-active{
	background-color: #3F3F3F;
	@include background-image(linear-gradient(#333 0%, #3F3F3F 100%)); 
	border: 1px solid #4E4E4E!important;
}

//form labels
.x-form-item-label{
	font-weight:bold;
}

//Chart-Viewer fixes
.xcp_gauge_chart-cls{
	.x-panel-header-default{
		background:#fff;
		border: none;
		//@include box-shadow(none);
		padding-left: 10px;
        height: auto;
	}
	.x-panel-body-default{
		border:none;
		@include box-shadow(none);
	}
}

.xcp-chart-inner-panel-body {
	@include box-shadow(none);
	
}

.xcp-not-supported img {
	opacity: 1!important;
}

.xcp-not-supported {
	color: #363636!important;
}


//Tabs
.xcp_tab_group-cls{

	.x-tab-bar{
		@include background-image(linear-gradient(#F8F8F8 30%, rgb(236,236,236) 94%) !important); 
		background-color: #F8F8F8 !important;
		border:		1px solid #dadada !important;
		@include border-top-radius(6px);
		border-bottom-width: 0;

	}
	
	
	.x-tab{
		border:		1px solid #dadada!important;
		@include border-top-radius(6px);
		height: 27px;
		background: #f6f6f6;
		
		button{
			color: #666;
			font-weight: normal;
			text-shadow:none;
		}
		&.x-tab-xcp-tab-ui-over{
			background: #fff !important;
			button{
			  color: #39C !important; 
			  text-shadow: none;
			  font-size: 12px;
			}
		}
	}
	
	.x-tab-xcp-tab-ui-active {
		background: #fff !important;
		border-bottom-width: 0 !important;
		
		button{
			color: #666;
			font-size: 12px;
			font-weight: bold;
			text-shadow:none;
		}
	}
	
	.x-tab-bar-top .xcp-tab-bar-caret{
		display: none;
	}
	
	.x-tab-bar-top .x-tab-bar-body {
		height: 29px;
	}
	
	.x-tab-bar-strip{
		background: #fff !important;
		border-color: #dadada !important;
	}
	
}

.xcp_tab_group-cls > .x-panel-body {
	background: white;
	border: 1px solid #dadada;
	@include border-bottom-radius(6px);
	border-top-width: 0;
}


//Buttons 
.x-btn-default-small.x-btn-default-small-noicon{
	border-radius: 6px;
	@include sassy-button("matte", 6px, 12px, #4ba3cc);
	//padding: 0.4em 1.1em !important;
	padding: 0.3em 1.1em !important;
	border-width: 1px !important;
	//box-shadow: 0 1px 2px #358FBA inset;
	
	
	&.x-btn-default-small-disabled{
		opacity: 0.6;
		background: #4ba3cc;
		cursor: default;
		@include box-shadow(none);
	}
	
	&.x-btn-default-small-focus{
		box-shadow: 0 1px 2px #358FBA inset;
	}
	
	&.x-btn .x-btn-center .x-btn-inner{
		padding: 0;
		font-size: 12px;
		font-family: inherit;
	}
}

//Call to Action button
.cta .x-btn-default-small.x-btn-default-small-noicon{
	@include sassy-button("matte", 6px, 16px, #8cbe5f);
	
	&.x-btn-default-small-disabled{
		opacity: 0.6;
		background: #8cbe5f;
		cursor: default;
		@include box-shadow(none);
	}
	
	&.x-btn .x-btn-center .x-btn-inner{
		padding: 0;
		font-size: 16px;
		font-family: inherit;
	}
}

.flat .x-btn-default-small.x-btn-default-small-noicon{

	background-color: #4ba3cc;
	border-radius: 4px;
	
	&.x-btn-default-small-disabled{
		opacity: 0.6;
		background: #4ba3cc;
		cursor: default;
		@include box-shadow(none);
	}
} 


//Sign In Page
.signin-panel{
	background: #EEE;
}

.signin-panel .signin-form .x-panel-body{
	background: #fff !important;
}

.signin-title {
	color: #4E9AC9;
}

#signin-page {
	margin: 0;
	padding: 0;
	border: 0 none;
	overflow: hidden;
	height: 10000px;
	position: static;
	background-color: #F5F5F5;
	//@include background-image(radial-gradient(center top, ellipse farthest-corner, #fff, #f5f5f5 70%));
}

/* LOGO Goes here
.signin-panel .signin-logo-panel .x-panel-body {
	background: url("../../../icons/welcome.png") no-repeat scroll 0 0 transparent;
	height: 50px;
} */

//form field fix
TABLE.x-form-item td{
     vertical-align: top;
}

.x-form-item-label,.x-form-display-field{
 padding-top: 3px;   
}

.xcp_folder_view-cls > .x-panel-body-default{
	border: none;
}

//Tree
.xcp_content_tree-cls {
	padding: 0;
	.x-grid-body {
		@include box-shadow(none);
		margin: 0;
		@include border-radius(6px);
		border: 1px solid #D7D7D7;
		
		TABLE{
			margin-top: 6px;
		}
		
		.xcp-content-runtime-tree-root div.x-grid-cell-inner {
			padding-left: 9px !important;
		}
		
		.x-tree-elbow-empty {
			width: 9px;
		}
	}
}

//Grid
.xcp_results_list-cls {
	margin: 0 !important;
	@include border-radius(6px);
	border: 1px solid #D7D7D7;
	.x-grid-header-ct{
		@include background-image(linear-gradient(#F8F8F8 30%, rgb(236,236,236) 94%));
		@include border-top-radius(6px);
		background-color: #F8F8F8;
	}
	
	.x-column-header{
		background: inherit;
		border: none;
	}
	
	.xcp_pagingtoolbar-cls {
		@include background-image(linear-gradient(#F8F8F8 30%, rgb(236,236,236) 94%)); 
		background-color: #F8F8F8;
		
	}
}

// .xcp_results_list-cls > .x-panel-body{
// 	@include border-radius(6px);
// }

.xcp_results_list-cls > .x-panel-body .xcp_results_list_grid-cls {
	@include box-shadow(none);
	margin: 0;
}


//Viewer
.rmViewerToolBar{
	background: inherit;
}
.xcp_viewer-cls {
	.x-panel-body.x-panel-body-default{
		@include box-shadow(none);
		@include border-radius(6px);	
	}
	
	.rmViewerToolBar{
			@include background-image(linear-gradient(#F8F8F8 30%, rgb(236,236,236) 94%));
			//background-color: #F8F8F8;
			@include border-top-right-radius(6px);
	}
	
	.pgModTbl .x-toolbar-default {
		background-color: rgb(236,236,236) !important;
	}
	
	.x-grid-table{
		background: #F8F8F8;
		.thmimg {
			border: 1px solid #CCC;
		}
	}
	
	.rmPageDataView  .rmPageContainer{
		background: #F8F8F8;
		img{
			border: 1px solid #CCC;
		}
	}
	
	.compPanel .x-panel-header-default{
		@include background-image(linear-gradient(#F8F8F8 30%, rgb(236,236,236) 94%) !important);
		background-color: #F8F8F8;
	}

}
// Attachment list
.xcp_attachment_grid-cls{
	> .x-panel-body-default{
		@include border-radius(6px);
	}
	
	.x-toolbar-docked-top{
		@include border-top-radius(6px);
	}
}

.x-toolbar-xcp-toolbar, .xcp_pagingtoolbar-cls, .xcp_results_list_grid-cls .x-toolbar-docked-top {
	@include background-image(linear-gradient(#F8F8F8 30%, rgb(236,236,236) 94%));
	background-color: #F8F8F8;
	height: auto;
}

.xcp_toolbar-cls, .xcp_results_list_grid-cls .x-toolbar-docked-top {
	border-bottom: 1px solid #D7D7D7 !important;
}

.xcp_pagingtoolbar-cls{
	@include border-bottom-radius(6px);
}

.x-toolbar-xcp-toolbar .x-btn, .rmViewerToolBar .x-btn, .xcp_results_list_grid-cls .x-toolbar-docked-top .x-btn {
	height: 26px;
}


//Error Messages

.x-mask-msg{
	background: inherit;
	border: none;
	
	DIV{
		border:none;
		background: inherit;
	}
	
	.x-mask-msg.xcp-mask-info div{
		background: inherit;
		cursor: inherit;
	}
}

//Comments

.xcp_comments-cls{
	.x-toolbar-default {
		height: 30px;
		@include background-image(linear-gradient(#F8F8F8 30%, rgb(236,236,236) 94%));
		background-color: #F8F8F8;
		@include border-top-radius(6px);
	}
	
	.x-panel .x-grid-body{
		@include border-bottom-radius(6px);
	}
}

//MultiValue Lists

.xcp_multivalueinput .x-grid-body{
	border: 1px solid #D7D7D7;
	border-top-width: 0;
}

.xcp_multivalueinput_toolbar{
	border-bottom-width: 0 !important;
}

.xcp_multivalue_input_grid-cls .x-grid-table {
	border-width: 1px 0 0 0!important;
}


.button-bar{
	 border-top: 1px solid #D7D7D7;
	 padding: 6px 0 0; 
	// margin-left: -5px!important;
	 //margin-right: -5px!important;
	 @include background-image(linear-gradient(#F8F8F8 30%, rgb(236,236,236) 94%));  
	 background-color: #F8F8F8;
	 @include border-bottom-radius(6px);
	 height: 37px!important;
}


